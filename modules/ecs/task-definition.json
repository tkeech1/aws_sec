[
  {
    "name": "bandit-Service",
    "image": "${ecr_image_tag}",
    "portMappings": [
      {
        "containerPort": 8000,
        "protocol": "http"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${bandit_log_group}",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "${bandit_log_group}"
      }
    },
    "environment": [
      {
        "name": "variable",
        "value": "value"
      }
    ],
    "essential": true
  }
]